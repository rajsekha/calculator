pipeline {
    agent any
    stages {
        stage ('Invoke_pipeline') {
            steps {	    
                build job: 'Quinnox-ios', parameters: [
                string(name: 'TEST_TYPE', value: "RELEASE")
		string(name: 'DEVICE_TYPE', value: "Android")
                ]
		triggers {
	            cron('18 17 * * *')
              }
            }
        }

        stage('End') {
            steps {
                echo 'ls'
            }
       }        
    }
}
        
