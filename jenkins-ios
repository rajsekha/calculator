
pipeline {
    agent any
    triggers {
	    cron('13 11 * * *')
    }
    stages {
        stage ('Invoke_pipeline') {
            steps {	    
                build job: 'Quinnox-ios', parameters: [string(name: 'DEVICE_TYPE', value: 'Android'), string(name: 'TEST_TYPE', value: 'RELEASE')]

            }
        }

        stage('End') {
            steps {
                echo 'ls'
            }
       }        
    }
}
