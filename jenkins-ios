pipeline {
    agent any
     parameters {
                string(name: 'TEST_TYPE', defaultValue: "RELEASE")
                string(name: 'DEVICE_TYPE', defaultValue: "iOS")
                }
    stages {
        stage ('Invoke_pipeline') {
            steps {	    
                build job: 'Quinnox-ios', parameters: ([] + params)
		}
        }       
    }
}
